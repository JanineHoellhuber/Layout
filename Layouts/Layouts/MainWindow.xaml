<Window x:Class="Layouts.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Layouts"
        mc:Ignorable="d"
        Title="MainWindow" Height="270" Width="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="50" />
            <RowDefinition Height="40" />
            <RowDefinition Height="50" />
            <RowDefinition Height="40" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>

        <Button x:Name="stackpanel" Grid.Row="1" Grid.Column="1" Content="StackPanel" Margin="5,5,5,5" />
        <Button x:Name="dock" Grid.Row="2" Grid.Column="1" Content="Dock" Margin="5,5,5,5" />
        <Button x:Name="grid" Grid.Row="3" Grid.Column="1" Content="Grid" Margin="5,5,5,5" />
        <Button x:Name="gridsplitter" Grid.Row="4" Grid.Column="1" Content="Grid Splitter" Margin="5,5,5,5" />

      
    </Grid>
</Window>
